---
layout: base.njk
tags: [setting]

pageTitle: Setting Name
thumbnailUrl: thumbnails/setting/example.png
pageThemes: []

name: Setting Name
title: Informational title

---

{% from "macros/_nav.njk" import tagNav, collectionNav %}

<article>
	<main>
		{{ content | safe }}
	</main>

	<footer>
		<h2 aria-labelledfor="what-links-here">What Links Here</h2>
		<ul id="what-links-here">
			{% for character in collections.all | whatLinksHere %}
				<li>
					<a href="{{character.url}}">{{character.data.pageTitle}}</a>
				</li>
			{% endfor %}
		</ul>

		{% set previous = collections.setting | alphabetizePages | getPreviousCollectionItem %}
		{% set next = collections.setting | alphabetizePages | getNextCollectionItem %}
		{{ collectionNav("Settings", "/settings", previous, next) }}
		
		{{ tagNav(tags) }}
	</footer>
</article>